        </If.Then>
      </If>
    </Sequence>
    <mtbwa:AgentScope DisplayName="Выполнение в агенте" MaxExecutionTime="[AgentSettings.MaxExecutionTime]" MaxWaitTime="[AgentSettings.MaxWaitTime]" ReservationSpec="[AgentSettings.GetAgentReservationSpec()]">
