          <If.Else>
            <Assign x:TypeArguments="mtbwa:PlatformConfiguration" DisplayName="Использовать конфигурацию платформы пользователя" To="[ConfigurationToBuild]" Value="[ConfigurationsToBuild(0)]" mtbwt:BuildTrackingParticipant.Importance="Low"/>
