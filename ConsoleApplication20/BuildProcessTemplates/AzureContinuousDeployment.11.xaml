                                          </Sequence.Variables>
                                          <mtbwa:ConvertWorkspaceItem DisplayName="Преобразование серверного пути в локальный" Input="[AlternatePublishProfile]" Result="[localProfile]" Workspace="[Workspace]" mtbwt:BuildTrackingParticipant.Importance="Low"/>
                                          <Assign x:TypeArguments="mtb:AzureCloudAppProfile" mtbwt:BuildTrackingParticipant.Importance="Low" DisplayName="Загрузить альтернативный профиль публикации" To="[azureCloudAppProfile]" Value="[AzureCloudAppProfile.Parse(System.IO.File.ReadAllText(localProfile))]"/>
