    <mtbwa:GetBuildDetail DisplayName="Получение построения" Result="[BuildDetail]" mtbwt:BuildTrackingParticipant.Importance="Low"/>
    <Sequence DisplayName="Обновление расположения сброса" mtbwt:BuildTrackingParticipant.Importance="Low">
