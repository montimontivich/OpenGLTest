              <Assign x:TypeArguments="x:String" DisplayName="Обрезать значение WorkspaceName до размера MaxSize" To="[WorkspaceName]" Value="[WorkspaceName.Substring(0, Microsoft.TeamFoundation.VersionControl.Common.RepositoryConstants.MaxWorkspaceNameSize).TrimEnd()]" mtbwt:BuildTrackingParticipant.Importance="Low"/>
            </Sequence>
          </If.Then>
        </If>
      </Sequence>
      <Sequence DisplayName="Инициализация рабочей области" mtbwt:BuildTrackingParticipant.Importance="Low">
        <mtbwa:DeleteDirectory Directory="[TestResultsDirectory]" DisplayName="Удаление каталога с результатами тестов" Recursive="[True]" mtbwt:BuildTrackingParticipant.Importance="Low"/>
        <If Condition="[Not CleanWorkspace = Microsoft.TeamFoundation.Build.Workflow.Activities.CleanWorkspaceOption.None]" DisplayName="Если CleanWorkspace не равно CleanWorkspaceOption.None" mtbwt:BuildTrackingParticipant.Importance="Low">
